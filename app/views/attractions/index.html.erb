<h1>Listing attractions</h1>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Tickets</th>
    <th>Details</th>
  </tr>
  
  <% @attractions.each do |a| %>
    <tr>
      <td><%= a.name %></td>
      <td><%= a.tickets %></td>
      <td><%= attractions_link(a) %></td>
    </tr>
  <% end %>
</table>

<% if current_user.admin == true %>
  <%= link_to "New Attraction", new_attraction_path %>
<% end %>

